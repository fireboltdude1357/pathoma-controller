{
  "manifest_version": 3,
  "name": "Pathoma Controller",
  "description": "Remote video control for pcloud.link",
  "version": "1.0.0",
  "permissions": ["storage", "alarms"],
  "host_permissions": [
    "*://*.pcloud.link/*",
    "*://u.pcloud.link/*"
  ],
  "background": {
    "service_worker": "dist/background.js",
    "type": "module"
  }
}
